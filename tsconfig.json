{
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "./assets/src",
        "sourceMap": false,
        "noImplicitAny": true,
        "target": "ES6",
        "lib": ["dom", "es6", "es2015.promise"],
        "module": "commonjs",
        "allowJs": true,
        "rootDirs": [
           "src"
        ],
        "experimentalDecorators": true
    },
    "include": [
        "src/VaangoUI.ts",
        "src/views/**/*",
        "src/vuex/**/*",
        "src/graphics/**/*"
    ],
    "exclude": [
        "node_modules",
        "assets"
    ]
}
